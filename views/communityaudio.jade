html
    head
        title Community - Audio
        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=1')
        link(rel='stylesheet', href='/stylesheets/main.css')
        link(rel='stylesheet', href='/stylesheets/style.css')
    body(onload="getAudioFile()").subpage
        // Header
        header#header
            .inner
                a.logo(href='homepage')
                    strong Home Media Center
                    |  by DAM Group
                nav#nav
                    a(href='personalcenter?id=#{id}') Personal Center
                a.navPanelToggle(href='#navPanel')
                    span.fa.fa-bars
        // Three
        section#three.wrapper
            .inner
                header.align-center
                    h2 Audio
                    p mp3 , wav
                // content goes here
                div(text-align='center')
                    ul.align-center
                        p.hide#audioBox=JSON.stringify(catalog)

        // Scripts
        script(src='/javascripts/jquery.min.js')
        script(src='/javascripts/skel.min.js')
        script(src='/javascripts/util.js')
        script(src='/javascripts/main.js')
        script(src='/javascripts/function.js')
        script(src='/javascripts/json2.js')
