html
    head
        title DAM Project - Home Page
        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=1')
        link(rel='stylesheet', href='/stylesheets/main.css')
        link(rel='stylesheet', href='/stylesheets/style.css')
    body
        // Header
        header#header
            .inner
                a.logo
                    strong Home Media Center
                    |  by DAM Group
                nav#nav
                    a 张豫元
                    a 朱丹瑜
                    a 章天乙
                    a 张月
                    a 赵云婷
                a(href="logout") 注销
                a.navPanelToggle(href='#navPanel')
                    span.fa.fa-bars
        // Banner
        section#banner
            .inner
                header
                    h1 Welcome to Home Media Center
                form(action='#', method='post')
                    ul.center
                        div
                            div.left
                                input.selectBox#textInput(type="text",name="wd")
                                input.selectBtn(type="submit",value="SELECT")
                .flex
                    div
                        h3
                            a(href='communityvideo') Video
                        p mp4 , avi
                    div
                        h3
                            a(href='communityaudio') Audio
                        p mp3 , wav
                    div
                        h3
                            a(href='communitytext') Text
                        p doc , txt
                    div
                        h3
                            a(href='communityimage') Image
                        p bmp
                if id
                    footer
                        a.button(href='personalcenter?id=#{id}') Enter Personal Center


        // Scripts



        script(src='/javascripts/jquery.min.js')
        script(src='/javascripts/skel.min.js')
        script(src='/javascripts/util.js')
        script(src='/javascripts/main.js')
        script(src='/javascripts/function.js')
        script(src='/javascripts/json2.js')
